<div style="display:none" id="rank-perf">
   <% current_user.tennis_perf.each do |tennis|%>
      <p>  <%= tennis.rank%> </p>
   <%end%>
</div>
<div style="display:none" id="date-perf-tennis">
   <% current_user.tennis_perf.each do |tennis|%>
      <p>  <%= tennis.created_at.strftime("%d/%m  %kh%M")%> </p>
   <%end%>
</div>
   <br>
<canvas id="tennisChart" width="100" height="100"></canvas>