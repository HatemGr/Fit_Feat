<div style="display:none" id="benchpress-perf">
   <% current_user.workout_perf.each do |workout|%>
      <p>  <%= workout.benchpress_weight%> </p>
   <%end%>
</div>

<div style="display:none" id="squat-perf">
   <% current_user.workout_perf.each do |workout|%>
      <p>  <%= workout.squat_weight%> </p>
   <%end%>
</div>

<div style="display:none" id="date-perf-workout">
   <% current_user.workout_perf.each do |workout|%>
      <p>  <%= workout.created_at.strftime("%d/%m  %kh%M")%> </p>
   <%end%>
</div>

<br>

<canvas id="workoutChart" width="100" height="100"></canvas>