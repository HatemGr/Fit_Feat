<div style="display:none" id="speed-perf">
   <% current_user.running_perf.each do |running|%>
      <p>  <%= running.speed%> </p>
   <%end%>
</div>

<div style="display:none" id="distance-perf">
   <% current_user.running_perf.each do |running|%>
      <p>  <%= running.distance%> </p>
   <%end%>
</div>

<div style="display:none" id="date-perf-running">
   <% current_user.running_perf.each do |running|%>
      <p>  <%= running.created_at.strftime("%d/%m  %kh%M")%> </p>
   <%end%>
</div>

<br>

<canvas id="runningChart" width="100" height="100"></canvas>