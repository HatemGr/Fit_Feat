    <div class="edit-profil-btn">
      <h1> Events 🏅</h1> 
      <%= link_to 'Crée un event', new_event_path, class:'button primary' %>
    </div>
    <%=render 'events_around'%>
    <br>
    <div class="card">
      <p class="card-title"> Fait vivre la communauté en organisants des propres events </p>
      <p class="card-icon"> <i class="fa-solid fa-calendar-plus"></i></i> </p> 
      <%= render partial: 'event_list', locals: {events: @current_user_events}%>
    </div>
</div>
